<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Talenttradingmarketkt" parent="Theme.AppCompat.Light">
        <item name="windowNoTitle">true</item>
        <item name="bottomNavigationStyle">@style/MyBottomNavigationView</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">#45BC81</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/white</item>
        <!-- Customize your theme here. -->

    </style>

    <style name="MyBottomNavigationView" parent="Widget.Design.BottomNavigationView">
        <!-- Customize the colors here -->
        <item name="android:background">@color/white</item>
        <item name="itemIconTint">#3DC181</item>
        <item name="itemTextColor">#3DC181</item>
    </style>
</resources>